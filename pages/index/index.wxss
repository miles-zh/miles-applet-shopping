/**index.wxss**/
.container {
  background-color: var(--themeColor);
}

.index-swiper swiper {
  height: 340rpx;
  width: 750rpx;
}

.index-swiper image {
  width: 100%;
}

.index-cate {
  display: flex;
}

.index-cate navigator {
  flex: 1;
  padding: 20rpx;
}

.index-cate navigator image {
  width: 100%;
}

.index-floor {}

.index-floor .floor-group {}

.index-floor .floor-group .floor-title {
  width: 100%;
  
}

.index-floor .floor-group .floor-list {}

.index-floor .floor-group .floor-title image {
  width: 100%;
  margin: 10rpx 0;
}

.index-floor .floor-group .floor-list navigator {
  float: left;
  width: 33.33%;
}

.index-floor .floor-group .floor-list navigator image {
  width: 100%;
  height: 100%;
  border-right: 10rpx solid #fff;
}
.index-floor .floor-group .floor-list navigator:nth-child(1){
  border-left: 10rpx solid #fff;
}
/* 后面4个链接的宽高 */
.index-floor .floor-group .floor-list navigator:nth-last-child(-n+4){
  height: calc( (33.33vw * 386) / (232 * 2) - 17rpx );
  
}
.index-floor .floor-group .floor-list navigator:nth-child(2),.index-floor .floor-group .floor-list navigator:nth-child(3){
  border-bottom: 10px solid #fff;
}
